{
  "name": "bofdearimgui",
  "version": "2.2.1.10",
  "description": "The OnBings DearImGui c++ wrapper library",
  "dependencies": [
    "bofstd",
    {
      "name": "hello-imgui",
      "features": [
        "opengl3-binding"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "opengl3-binding"
      ]
    },
    "implot",
    {
      "name": "sdl2-core",
      "default-features": false
    },
    {
      "name": "sdl2-core",
      "default-features": false,
      "features": [
        "alsa",
        "x11"
      ],
      "platform": "!windows"
    }
  ]
}
