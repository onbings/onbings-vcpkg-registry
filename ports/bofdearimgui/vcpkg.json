{
  "name": "bofdearimgui",
  "version": "2.2.1.7",
  "description": "The OnBings DearImGui c++ wrapper library",
  "dependencies": [
    {
      "name": "bofstd"
    },
    {
      "name": "hello-imgui",
      "features": [
        "opengl3-binding"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "opengl3-binding"
      ]
    },
    {
      "name": "sdl2",
      "default-features": false
    },
    {
      "name": "sdl2",
      "default-features": false,
      "features": [
        "alsa"
      ],
      "platform": "!windows"
    },
    {
      "name": "implot"
    }
  ],
  "port-version": 0
}