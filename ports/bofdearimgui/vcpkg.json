{
  "name": "bofdearimgui",
  "version": "2.2.1.9",
  "description": "The OnBings DearImGui c++ wrapper library",
  "dependencies": [
    {
      "name": "bofstd"
    },
    {
      "name": "hello-imgui",
      "features": [
        "opengl3-binding"
      ]
    },
    {
      "name": "imgui",
      "features": [
        "opengl3-binding"
      ]
    },
    {
      "name": "sdl2-core",
      "default-features": false
    },
    {
      "name": "sdl2-core",
      "default-features": false,
      "features": [
        "alsa",
		"x11"
      ],
      "platform": "!windows"
	},
    {
      "name": "implot"
    }
  ],
  "port-version": 3
}